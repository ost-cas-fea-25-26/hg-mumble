{
  "name": "hg-mumble",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env PGSSLMODE=require NODE_TLS_REJECT_UNAUTHORIZED=0 next dev --turbopack",
    "dev:mock": "cross-env PGSSLMODE=require NODE_TLS_REJECT_UNAUTHORIZED=0 API_URL=http://localhost:4011 next dev --turbopack",
    "mock": "prism mock scripts/generate/swagger_mumble_api.json --port 4011",
    "build": "next build --turbopack",
    "start": "cross-env PGSSLMODE=require NODE_TLS_REJECT_UNAUTHORIZED=0 next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate": "node --loader ts-node/esm scripts/generate/generateApis.ts",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "e2e": "npm run test"
  },
  "dependencies": {
    "@supabase/ssr": "^0.8.0",
    "ahooks": "^3.9.6",
    "better-auth": "^1.3.34",
    "clsx": "^2.1.1",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "fslightbox-react": "^2.0.0",
    "hg-storybook": "^1.26.1",
    "next": "16.1.1",
    "next-intl": "^4.5.3",
    "pg": "^8.16.3",
    "playwright": "^1.56.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.66.1",
    "sonner": "^2.0.7",
    "ulidx": "^2.4.1",
    "use-intl": "^4.5.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.56.1",
    "@stoplight/prism-cli": "^5.14.2",
    "@tailwindcss/postcss": "^4",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/fslightbox-react": "^1.8.0",
    "@types/node": "^24.0.0",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "knip": "^5.69.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "swagger-typescript-api": "^13.2.16",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
